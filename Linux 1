 function lc {   ls Emails_n* |cut -d_ -f4-6 |sort |uniq >emails_datas;    while  read j ;   do    ls *$j >tmp_d1; >tmp_d12;    while  read k ;       do    cat $k >>tmp_d12;    done<tmp_d1;    mv tmp_d12 "Unique_$j";  sort -uo  "Unique_$j"  "Unique_$j";  wc -l "Unique_$j";    done<emails_datas;    }
function b {      >tmp_use1;   >tmp_files;   sum=0;    while read i; do    sum=`expr $sum + $i`;    done<$2;     sort -uo $1 $1;   count=$(wc -l $1|cut -d" " -f1);    if [ $sum -le $count ] ;    then  hed=0 ;  tal=0;z=1;   for i in `cat $2` ;   do tal=$i ;    hed=`expr $hed + $i`;   head '-'$hed $1 | tail '-'$tal > $1'_h'$hed'_t'$tal ;    fgrep -xvf tmp_use1  $1'_h'$hed'_t'$tal >new_tmp1;   mv new_tmp1  $1'_h'$hed'_t'$tal;    dname=$(sed -e "$z q;d" $3|cut -d, -f1);   lname=$4;   tdate=$(date +%d%m);   fcount=$(wc -l $1'_h'$hed'_t'$tal|cut -d" " -f1);   dcount=$(sed -e "$z q;d" $3|cut -d, -f2);   z=$(($z + 1));   if [ $fcount -eq $(($dcount)) ] ;     then     cp $1'_h'$hed'_t'$tal 'Emails_n'$fcount'_'$dname'_'$tdate'_'$4;   wc -l 'Emails_n'$fcount'_'$dname'_'$tdate'_'$4;   else   tput setaf 1;   echo "file counts not matched";   tput setaf 7;    fi;   cat $1'_h'$hed'_t'$tal  >>tmp_use1; echo 'Emails_n'$fcount'_'$dname'_'$tdate'_'$4 >>prolist;  echo $1'_h'$hed'_t'$tal>>tmp_files;     done;  fgrep -xvf tmp_use1 $1 >$1'_final';    wc -l $1'_final';   rm tmp_use1;   else    tput setaf 1;      echo ""file count is less;     tput setaf 7;    fi;     }
function pro {  > kpro; vi kpro;>prolist;  sed -i 's/\t/,/g' kpro;   C=`cat kpro | head -1 | sed 's/,/\n/g' | wc -l`; for (( j=5; j<=$C; j++ )); do cut -d, -f1,$j kpro>tmp_$j;  >tmp1_$j;  while read k;  do   if [ ! -z $(echo $k |cut -d, -f2) ] ;      then     echo $k>>tmp1_$j;   fi;    done<tmp_$j;   listname=$(cut -d, -f2 tmp1_$j|head -1);      tput setaf 2;     echo "----- $listname -------"; tput setaf 7;     list=$(cut -d, -f2 tmp1_$j|sed '2q;d');    cut -d, -f2 tmp1_$j |sed 1,2d >tmp_count_$j; sed 1,2d  tmp1_$j >tmp_doms_$j;    b $list tmp_count_$j  tmp_doms_$j $listname;  rm tmp1_$j tmp_$j tmp_count_$j  tmp_doms_$j; done;   }
function pro1 {  > kpro;>prolist; vi kpro;  sed -i 's/\t/,/g' kpro;   C=`cat kpro | head -1 | sed 's/,/\n/g' | wc -l`; for (( j=$1; j<=$2; j++ )); do cut -d, -f1,$j kpro>tmp_$j;  >tmp1_$j;  while read k;  do   if [ ! -z $(echo $k |cut -d, -f2) ] ;      then     echo $k>>tmp1_$j;   fi;    done<tmp_$j;   listname=$(cut -d, -f2 tmp1_$j|head -1);      tput setaf 2;     echo "----- $listname -------"; tput setaf 7;     list=$(cut -d, -f2 tmp1_$j|sed '2q;d');    cut -d, -f2 tmp1_$j |sed 1,2d >tmp_count_$j; sed 1,2d  tmp1_$j >tmp_doms_$j;    b $list tmp_count_$j  tmp_doms_$j $listname;  rm tmp1_$j tmp_$j tmp_count_$j  tmp_doms_$j; done;   }
