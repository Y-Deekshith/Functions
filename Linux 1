 function sa {     if [ -f safile ] ;    then   echo "please traing in few minutes"; else  >pfile;  >colm;  vim colm;  sed -i 's/\t/|/g' colm ;  count=0;  file="";  while read i; do   count=$(echo $i|cut -d"|" -f1); file=$(echo $i|cut -d"|" -f3); head  '-'$count $file >$file'_H'$count; wc -l $file'_H'$count;  done<colm;   fi; }
function sp {    if [ -f spfile ] ;   then      echo "please wait a min or you may remove spfile ";  else   >spfile;  splityy=$RANDOM ; >tmp_used;  > $splityy ;vim $splityy; sum=0; while read i; do sum=`expr $sum + $i`;  done<$splityy; sort -uo $1 $1; count=$(wc -l $1|cut -d" " -f1); if [ $sum -le $count ] ;   then  hed=0 ;  tal=0; for i in `cat $splityy` ;  do tal=$i ;  hed=`expr $hed + $i`;  head '-'$hed $1 | tail '-'$tal > $1'_h'$hed'_t'$tal ;  fgrep -xvf tmp_used  $1'_h'$hed'_t'$tal >new_tmp; mv new_tmp  $1'_h'$hed'_t'$tal;  wc -l $1'_h'$hed'_t'$tal ;  cat $1'_h'$hed'_t'$tal  >>tmp_used;  fgrep -xvf tmp_used $1 >$1'_final';  done; wc -l $1'_final'; rm tmp_used;  rm $splityy;  else echo ""file count is less;   fi;   rm  spfile; fi;   }
function sp1 {    if [ -f sp1file ] ;   then      echo "please wait a min or you may remove sp1file ";  else   >sp1file;  splityy=$RANDOM ; >tmp_use1;  > $splityy ;vim $splityy; sum=0; while read i; do sum=`expr $sum + $i`;  done<$splityy; sort -uo $1 $1; count=$(wc -l $1|cut -d" " -f1); if [ $sum -le $count ] ;   then  hed=0 ;  tal=0; for i in `cat $splityy` ;  do tal=$i ;  hed=`expr $hed + $i`;  head '-'$hed $1 | tail '-'$tal > $1'_h'$hed'_t'$tal ;  fgrep -xvf tmp_use1  $1'_h'$hed'_t'$tal >new_tmp1; mv new_tmp1  $1'_h'$hed'_t'$tal;  wc -l $1'_h'$hed'_t'$tal ;  cat $1'_h'$hed'_t'$tal  >>tmp_use1;  fgrep -xvf tmp_use1 $1 >$1'_final';  done; wc -l $1'_final'; rm tmp_use1;  rm $splityy;  else echo ""file count is less;   fi;   rm  sp1file; fi;   }
