 function sp {  splityy=$RANDOM ; >tmp_used;  > $splityy ;vim $splityy; sum=0; while read i; do sum=`expr $sum + $i`;  done<$splityy; sort -uo $1 $1; count=$(wc -l $1|cut -d" " -f1); if [ $sum -le $count ] ;   then  hed=0 ;  tal=0; for i in `cat $splityy` ;  do tal=$i ;  hed=`expr $hed + $i`;  head '-'$hed $1 | tail '-'$tal > $1'_h'$hed'_t'$tal ;  fgrep -xvf tmp_used  $1'_h'$hed'_t'$tal >new_tmp; mv new_tmp  $1'_h'$hed'_t'$tal;  wc -l $1'_h'$hed'_t'$tal ;  cat $1'_h'$hed'_t'$tal  >>tmp_used;  fgrep -xvf tmp_used $1 >$1'_final';  done; wc -l $1'_final'; rm tmp_used;  rm $splityy;  else echo ""file count is less;   fi; }
