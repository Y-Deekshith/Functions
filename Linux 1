function cu { tput setaf 2 ; cut -d, -f1 $1 | sort | uniq -c; tput setaf 7; }
function ckvcli { if [ $1 ] && [ $2 ] ; then > 'TOTAL_CLICKS_'$1'_'$2'DAYS' ; tput setaf 2 ; echo Getting $1 Offer $2 days Clicks From Cake ........... ; tput setaf 7 ; DDAY=`date +%m/%d/%Y` ; for ((i=0;i<=$2;i++)); do DDAY2=`date +%m/%d/%Y --date " - $i day"` ; done ; wget -qO- --load-cookies /home/moon/zcookies.txt --post-data "search_ip_addresses=&search_by_request_id=&date_range=custom&start_date=$DDAY2&end_date=$DDAY&offer=&isp=&disposition=&clicks_with_dups=1&test=-1&exclude_bot_traffic=0&o=click_date&d=ASC&report_id=17&report_view_id=17&csv=1" "http://zetaaffiliates.com/Extjs.ashx?s=reportclicks" | cut -d, -f11,13 | sed 's/"//g' | grep "[0-9]" | grep -i $1 | cut -d, -f1 | grep -iv "[a-z]"  >> 'TOTAL_CLICKS_'$1'_'$2'DAYS' ;  tput setaf 3 ; wc -l 'TOTAL_CLICKS_'$1'_'$2'DAYS' ; tput setaf 7;else tput setaf 2 ; echo Please Input Days ; tput setaf 7 ; fi  }
function ckvcli { if [ -f $1 ] && [ $1 ] ; then > VCLI999 ; for i in `cat $1` ; do D1=`echo $i | cut -d, -f1` ; D2=`echo $i | cut -d, -f2` ; wget -qO- --load-cookies /home/moon/cookies/183cookies.txt --post-data "search_ip_addresses=&search_by_request_id=&date_range=mtd&start_date=$D1&end_date=$D2&offer=&isp=&clicks_with_dups=0&test=-1&exclude_bot_traffic=0&o=click_date&d=ASC&report_id=17&report_view_id=17&csv=1" "http://zetaaffiliates.com//Extjs.ashx?s=reportclicks" "http://zetaaffiliates.com//Extjs.ashx?s=reportclicks" >> VCLI999 ; done ; sed -i  's/"//g' VCLI999 ; cut -d, -f11,13 VCLI999 > VCLI999_DATA  ; wc -l VCLI999_DATA  ; else tput setaf 2  ; echo Please give Multiple of 7 days in input file Like [ 5/18/2017,5/25/2017 ] ; tput setaf 7 ; fi  }
function unm { if [ $1 ] && [ $2 ] ; then  cut -d, -f3 $1 > $1'_EM' ; cat $1_'EM' |sort | uniq -c | sort -nr | grep -o "[0-9].*" | egrep "^1 |^2 |^3 " | cut -d' ' -f2  > $1'_Match' ; sort -uo $1'_Match' $1'_Match' ; sort -t, -uk3,3 -o $2 $2 ; join -t, -1 1 -2 3 -o 2.1 2.2 2.3 $1'_Match' $2 > $2'_Clean' ; wc -l  $2'_Clean' ; rm $1'_EM'  ; else tput setaf 2 ; echo Please Give 2 Files ; tput setaf 7 ;  fi ; cat $2'_Clean'  >> $1; }
function ui { cut -d, -f3 $1 > $1'_EMAILS' ; cut -d, -f3 $2 > $2'_EMAILS' ; sort -uo $1'_EMAILS' $1'_EMAILS' ; sort -uo $2'_EMAILS' $2'_EMAILS' ; fgrep -xvf $1'_EMAILS' $2'_EMAILS' > $2'_EMAILS_CLEAN' ; join -t, -1 1 -2 3 -o 2.1 2.2 2.3 $2'_EMAILS_CLEAN' $2 > $2'_CLEAN' ; wc -l $2 ; wc -l $2'_CLEAN' ; rm $1'_EMAILS' ; rm $2'_EMAILS' $2'_EMAILS_CLEAN' ; }
function ly { > CPM_DEP;> KK ;vi KK;sort -uo $1 $1 ; for i in `cat KK`;do sort -t, -uk2,2 -o $i $i ;join -v1 -t,  -1 2 -2 1 -o 1.1 1.2 1.3 $i $1 > $i'_UO';fgrep -xvf CPM_DEP $i'_UO' > $i'_Final';cat $i'_Final' >> CPM_DEP;rm -rf $i'_UO';wc -l $i'_Final' ; done }
function gd { lla=$RANDOM ; llo=$RANDOM; if  [ `pwd` = /home/moon/PFM_CUSTOMSCRIPTS/MOON_SUPPRESS/REPORT/`date +%Y%m%d` ] && [ $1 ];then > $lla ; vim $lla ; for i in `cat $lla` ; do if (sshpass -p 'moon$1209' ssh moon@66.161.124.122 "[ -f /data2/moon/MOON_UNIQUE_LISTS/FINAL_FILES/Dustbin_MoonB/$i ]"); then sshpass -p 'moon$1209' ssh moon@66.161.124.122 "cat /data2/moon/MOON_UNIQUE_LISTS/FINAL_FILES/Dustbin_MoonB/$i | sort | uniq " > $i'_temp' ;sort -t, -uk3 -o $1 $1; join -t, -1 1 -2 3 -o 2.1 2.2 2.3 $i'_temp' $1 >  $1'_'$i ; wc -l $1'_'$i ; rm $i'_temp' ;else tput setaf 1 ; echo File is not in Dustbin ; tput setaf 7 ;fi ; done ; else tput setaf 1 ;echo "Please Give Input || Wrong Path "   ;tput setaf 7; fi }
function gdd { lla=$RANDOM ; llo=$RANDOM; if  [ `pwd` = /home/moon/PFM_CUSTOMSCRIPTS/MOON_SUPPRESS/REPORT/`date +%Y%m%d` ] && [ $1 ];then if (ssh moon@66.161.124.122 "[ -f /data2/moon/MOON_UNIQUE_LISTS/FINAL_FILES/Dustbin_MoonA/$1 ]"); then > $lla ; vim $lla ; ssh moon@66.161.124.122 "cat /data2/moon/MOON_UNIQUE_LISTS/FINAL_FILES/Dustbin_MoonA/$1 | sort | uniq " > $1'_temp' ; for i in `cat $lla` ; do sort -t, -uk3 -o $i $i ; join -t, -1 1 -2 3 -o 2.1 2.2 2.3 $1'_temp' $i > $i'_'$1 ; wc -l $i'_'$1 ; done ; else tput setaf 1 ; echo File is not in Dustbin ; tput setaf 7 ;fi ; else tput setaf 1 ;echo "Please Give Input || Wrong Path "   ;tput setaf 7; fi }
function a { perl -e 'print "Enter Cated File:\n"; while (<STDIN>) {last if /^$/ ; push(@a,"/home/moon/kumar/FILES/$_"); } ; if($ARGV[0]) { foreach(@a){ chomp $_ ; system("cat $_ >> /home/moon/kumar/FILES/$ARGV[0]") ; } ; foreach(1..8){ system ("cat /home/moon/kumar/FILES/$ARGV[0]  > /home/moon/PFM_CUSTOMSCRIPTS/MOON_SUPPRESS/ADHOC$_/configFilePath/$ARGV[0]") ; } } else { print "======\n" ; foreach(@a){ chomp $_ ; system("cat $_ ") ; } }' $1 ; }
function sup { PA=/home/moon/PFM_CUSTOMSCRIPTS/MOON_SUPPRESS/MANNUAL_SUPP ; if [ -d $PA ] ; then  XML=`curl -s "http://www.ckadmin.com/affiliates/api/2/offers.asmx/GetSuppressionList?api_key=ErS7or2ONI&affiliate_id=183&offer_id=$1" |  grep "<download_url>" | cut -d'>' -f2 | cut -d'<' -f1` ;  if [ `echo $XML | grep -c mailer.optizmo.net` == 1 ]; then key=`echo $XML | cut -d'>' -f2 | cut -d'<' -f1 | cut -d'/' -f4 | cut -d'&' -f1` ; link=`curl -s -k  "https://mailer_api.optizmo.net/accesskey/download/$key?token=F7m8ryNA7CSHJhNRAjuiBRMGunYPxfH9&format=plain&deltas=0&delta_timestamp=0"  | cut -d',' -f6 | cut -d':' -f2,3 | sed 's/"//g' ` ; if [ "$link" == "" ] ; then link=`curl -s -k  "https://mailer_api.optizmo.net/accesskey/download/$key?token=F7m8ryNA7CSHJhNRAjuiBRMGunYPxfH9&format=md5&deltas=0&delta_timestamp=0"  | cut -d',' -f6 | cut -d':' -f2,3 | sed 's/"//g'` ; fi ; PA=/home/moon/PFM_CUSTOMSCRIPTS/MOON_SUPPRESS/MANNUAL_SUPP ; cd $PA ;aa=$RANDOM ;mkdir $PA/$aa ;cd $PA/$aa ; `lftpget $link` ;zipfile=`ls $PA/$aa | tail -1` ; unzip -qq $PA/$aa/$zipfile | grep afd ;supfile=`ls $PA/$aa | grep -i ^suppre`; dos2unix $PA/$aa/$supfile; cat $PA/$aa/$supfile > $PA/$supfile ;  cd $PA ; rm -rf $PA/$aa ; scp $PA/$supfile moon@66.161.124.122:/home/moon/PFM_CUSTOMSCRIPTS/MOON_SUPPRESS/MANNUAL_SUPP/ ; echo $supfile ; rm -rf $PA/$supfile ; elif [ `echo $XML | grep -c unsubcentral` == 1 ]; then url=`echo $XML | cut -d'>' -f2 | cut -d'<' -f1 | sed 's/\&amp;/\&/g'` ; effectiveurl=`curl -Ls -o /dev/null -w %{url_effective} $url` ; key=`echo "$effectiveurl" | cut -d'=' -f2` ; urlprefix=`echo "$effectiveurl" | cut -d'/' -f3` ; link=`echo "https://$urlprefix/api/ListDownload.html?keyID=$key&format=md5&zipped=true"` ; PA=/home/moon/PFM_CUSTOMSCRIPTS/MOON_SUPPRESS/MANNUAL_SUPP ; cd $PA ;aa=$RANDOM ;mkdir $PA/$aa ;cd $PA/$aa ; wget "$link" ; zipfile=`ls $PA/$aa | tail -1` ; unzip -qq $PA/$aa/$zipfile ; rm -rf $PA/$aa/$zipfile ;  supfile=`ls $PA/$aa | tail -1` ; dos2unix $PA/$aa/$supfile; cat $PA/$aa/$supfile > $PA/$supfile ;  cd $PA ; rm -rf $PA/$aa ; scp $PA/$supfile moon@66.161.124.122:/home/moon/PFM_CUSTOMSCRIPTS/MOON_SUPPRESS/MANNUAL_SUPP/ ; echo $supfile ;  rm -rf $PA/$supfile ;  else tput setaf 2 ; echo Offer ID Does\'nt Exist ;tput setaf 7 ; fi ;  else mkdir $PA ; sup $1 ; fi }
function spli { splityy=$RANDOM ; > $splityy ;vim $splityy;hed=0  ; tal=0; for i in `cat $splityy` ;do tal=$i ; hed=`expr $hed + $i`;  head '-'$hed $1 | tail '-'$tal  > $1'_h'$hed'_t'$tal ;echo -e 'LIST_ID,PROFILE_ID,EMAIL_ID' | cat - $1'_h'$hed'_t'$tal > temp && mv temp $1'_h'$hed'_t'$tal;echo $1'_h'$hed'_t'$tal ; done; rm $splityy; }
function ckocli { if [ $1 ] && [ $2 ] ; then > 'TOTAL_CLICKS_'$1'_'$2'DAYS' ; tput setaf 2 ; echo Getting $1 Offer $2 days Clicks From Cake ........... ; tput setaf 7 ; for ((i=0;i<=$2;i++)); do DDAY=`date +%m/%d/%Y --date " - $i day"` ; curl   -s "http://ckadmin.com/affiliates/api/1/reports.asmx/Clicks?api_key=ErS7or2ONI&start_date=$DDAY+00:00:00&end_date=$DDAY+23:59:59&disposition_type=&currency_id=0&include_duplicates=true&exclude_bot_traffic=false&conversion_type=conversions&affiliate_id=183&offer_id=$1&campaign_id=0&start_at_row=1&row_limit=100000" >> 'TOTAL_CLICKS_'$1'_'$2'DAYS' ; done ; grep subid_3 'TOTAL_CLICKS_'$1'_'$2'DAYS' | cut -d'>' -f2 | cut -d'<' -f1 | grep . | grep ^[0-9] > 'TOTAL_CLICKS_'$1'_'$2'DAYS_CLEAN' ; mv 'TOTAL_CLICKS_'$1'_'$2'DAYS_CLEAN' 'TOTAL_CLICKS_'$1'_'$2'DAYS' ; tput setaf 3 ; wc -l 'TOTAL_CLICKS_'$1'_'$2'DAYS' ; tput setaf 7;else tput setaf 2 ; echo Please Input Days ; tput setaf 7 ; fi  }

function ckcli { if [ $1 ] ; then  > 'TOTAL_CLICKS_'$1 ; tput setaf 2 ; echo Getting $1 days Clicks From Cake ........... ; tput setaf 7 ; for ((i=1;i<=$1;i++)); do DDAY=`date +%m/%d/%Y --date " - $i day"` ; curl   -s "http://ckadmin.com/affiliates/api/1/reports.asmx/Clicks?api_key=ErS7or2ONI&start_date=$DDAY+00:00:00&end_date=$DDAY+23:59:59&disposition_type=&currency_id=0&include_duplicates=true&exclude_bot_traffic=false&conversion_type=conversions&affiliate_id=183&offer_id=0&campaign_id=0&start_at_row=1&row_limit=100000" >> 'TOTAL_CLICKS_'$1 ;  done ; grep subid_3 'TOTAL_CLICKS_'$1 | cut -d'>' -f2 | cut -d'<' -f1 | grep . | grep ^[0-9] > 'TOTAL_CLICKS_'$1'_CLEAN' ; mv 'TOTAL_CLICKS_'$1'_CLEAN' 'TOTAL_CLICKS_'$1 ; tput setaf 3 ; wc -l 'TOTAL_CLICKS_'$1 ; tput setaf 7;else tput setaf 2 ; echo Please Input Days ; tput setaf 7 ; fi  }
function nh { echo 'EMAIL,NAME,EMAIL_ID' | cat - $1 > temp && mv -f temp $1 ; }
function te1 { echo 'newgmrpro@gmail.com,12,newgmrpro@gmail.com' | cat - $1 > temp && mv -f temp $1 ; }
function net {    sort -t, -uk2,2 -o $1 $1 ;   cut -d, -f2 $1 >$1'_pids';    sort -uo $1'_pids' $1'_pids';  join -t, -1 1 -2 2 -o 2.3 2.2 2.3 $1'_pids' $1 >$1'_netfinal';  te1 $1'_netfinal'; nh  $1'_netfinal';wc -l $1'_netfinal';    }
function nh { echo 'EMAIL,NAME,EMAIL_ID' | cat - $1 > temp && mv -f temp $1 ; }
