function CPA_F { > CPA_F_DEP; > RR ; vi RR ; sort -uo $1 $1 ; for i in `cat RR`;do sort -t, -uk2,2 -o $i $i ; join -t, -1 2 -2 1 -o 1.1 1.2 1.3 $i $1 > $i'_UO'; fgrep -xvf CPA_F_DEP $i'_UO' > $i'_FINAL';cat $i'_FINAL' >> CPA_F_DEP ; rm -rf $i'_UO';wc -l $i'_FINAL' ; done }


