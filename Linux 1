function d { if [ -f dom ] ;
then echo "please wait a min or you may or rm dom ";
else >dom;
>dm1; tput setaf 3;
line2="l1"; until [ "$line2" = "" ];
do read line2;
echo $line2>>dm1;
done; tput setaf 2;
tput setaf 7;
d=$(sed ':a;N;$!ba;s/\n/|/g' dm1);
r="${d%?}";
wc -l *$1*| sort -nr | egrep -i "$r";
rm dom ; fi; }



