PA/$aa/$supfile > $PA/$supfile ;  cd $PA ; rm -rf $PA/$aa ; scp $PA/$supfile moon@66.161.124.122:/home/moon/PFM_CUSTOMSCRIPTS/MOON_SUPPRESS/MANNUAL_SUPP/ ; echo $supfile ;  rm -rf $PA/$supfile ;  else tput setaf 2 ; echo Offer ID Does\'nt Exist ;tput setaf 7 ; fi ;  else mkdir $PA ; sup $1 ; fi }



