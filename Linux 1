function un { if [ $1 ] && [ $2 ] ; then  cut -d, -f2 $1 > $1'_EM' ; cat $1_'EM' |sort | uniq -c | sort -nr | grep -o "[0-9].*" | egrep -v "^1" | cut -d' ' -f2 | grep @ > $1_'DEP' ; cut -d, -f2 $2 > $2'_EM' ; fgrep -xvf $1'_EM' $2'_EM' > $2'_CLEAN' ; sort -uo $2'_CLEAN' $2'_CLEAN' ; sort -t, -uk2,2 -o $2 $2 ; join -t, -1 1 -2 2 -o 2.1 2.2 2.3 $2'_CLEAN' $2 > $2'_Final_Clean' ;  wc -l $2'_Final_Clean' ; rm $1'_EM'  $2'_EM' $2'_CLEAN' ; else tput setaf 2 ; echo Please Give 2 Files ; tput setaf 7 ;  fi ;  }
