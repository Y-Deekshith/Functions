function cpu {   ns3=$(date +%4N); >$ns3;vi $ns3; while  read j ;  do  wc -l $j; done<$ns3; d=$(sed ':a;N;$!ba;s/\n/ /g' $ns3);   sshpass -p'fZbxQfy6mzeko)4g' scp $d zx_isft@154.27.73.94:/home/zx_isft/`date +%Y%d%m`/production/cpcunclim;  rm $ns3;  }
function fpl { sshpass -p'Gmail@#$123' scp $1 gmail_FP@154.27.73.94:/home/gmail_FP/mailingdata/`date +%d%m`; }
function 1st {     ns4=$1;  while  read j ;   do     hdr $j;    wc -l $j;   done<$ns4;   d1=$(sed ':a;N;$!ba;s/\n/ /g' $ns4);  sshpass -p'Gmail@#$123' scp $d1 gmail_FP@154.27.73.94:/home/gmail_FP/mailingdata/`date +%d%m`/;   rm $ns4;  }
function sghgy {    ns3=$(date +%4N); >$ns3;vi $ns3; while  read j ;  do   wc -l $j; done<$ns3;  d=$(sed ':a;N;$!ba;s/\n/ /g' $ns3); sshpass -p'Gmail@#4$h@Y' scp $d gmaily@154.27.73.94:/home/gmaily/mailingdata/`date +%d%m`/production ;  rm $ns3;  }

function phead { head -$2 $1 > $1_h_$2; wc -l $1_h_$2; }
function ptail { tail -$2 $1 > $1_t_$2; wc -l $1_t_$2; }
function fh { gaa=$RANDOM ;  > $gaa ;  vi $gaa ; cat $gaa > gghygh  ; sed -i "s/,//g" gghygh ; sed -i "s/^/phead /g" gghygh ; . gghygh ; }
function ft { aa=$RANDOM ;  > $aa ;  vi $aa ; cat $aa > ghyg  ; sed -i "s/,//g" ghyg ; sed -i "s/^/ptail /g" ghyg ; . ghyg ; }
#function fc { ab=$RANDOM ; > $ab ; vi $ab; line=`tr '\n' '\t' < $ab`  ; wc -l $line; }
function fc { ab=$RANDOM ; > $ab ; vi $ab; sed -i "s/\n/\t/g" $ab ; wc -l `cat $ab`; }
function csv { cs=$RANDOM ; > $cs ; vi $cs ; while read filename ; do cp $filename $filename.csv ; hdr $filename.csv ; wc -l $filename.csv ; done<$cs ; }














